{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off",
    "func-names": "off",
    "no-new-func": "off",
    "prefer-arrow-callback": "off"
  },
  "globals": {
    "Page": true,
    "Network": true,
    "Runtime": true
  }
}
